page {
  background: #F5F6F7;
  padding-bottom: 120rpx;
}
page .title {
  margin-top: 20rpx;
  font-size: 36rpx;
  font-weight: bold;
}
.lists{
  padding: 10px 10px;
}
.lists .card{
  background: #fff;
  border-radius: 10px;
  padding: 5px 5px;
  margin: 0 0 15px 0;
  height: 123px;
}
.desc-info{
  padding: 0 15px;
  font-size: 14px;
  font-weight: bold;
  color: #666;
  line-height: 25px;
  text-align: center;
}
.desc-info .txt{
  display: block;
}
.ewm{
  padding: 0 15px;
  margin: 20px 0 20px 0;
  text-align: center;
}
.ewm .img{
  width: 130px;
  height: 130px;
}
